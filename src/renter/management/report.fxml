<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="Profile" minWidth="484.0" prefHeight="600.0" prefWidth="800.0" styleClass="background" stylesheets="@themeblue.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="renter.management.ReportController">
   <children>
      <AnchorPane prefHeight="529.0" prefWidth="800.0" AnchorPane.bottomAnchor="71.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <DatePicker fx:id="datePicker" editable="false" layoutX="8.0" layoutY="11.0" promptText="mm/yyyyy" />
            <TableView fx:id="tableReport" layoutX="14.0" layoutY="53.0" prefHeight="476.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="53.0">
              <columns>
                <TableColumn fx:id="colSl" prefWidth="63.0" text="SL" />
                <TableColumn fx:id="colName" prefWidth="176.0" text="Name" />
                  <TableColumn fx:id="colHouseRent" maxWidth="112.0" minWidth="-1.0" prefWidth="96.0" text="House Rent" />
                  <TableColumn fx:id="colBillMonth" prefWidth="107.0" text="Billing Month" />
                  <TableColumn fx:id="colBillPaid" minWidth="0.0" prefWidth="96.0" text="Bill Paid" />
                  <TableColumn fx:id="colDueBill" prefWidth="109.0" text="Due Bill" />
